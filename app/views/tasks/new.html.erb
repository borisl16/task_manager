<%= link_to("Back to list", tasks_path, :class => 'back-list') %>

<div class="tasks new"> 
    <h2>Create Task</h2>

    <%= form_for(@task) do |f| %>

        <%= render(partial: 'form', locals: {f: f})%>

        <div class="form-buttons">
            <%= f.submit("Create Task") %>
        </div>

    <% end %>
</div>
